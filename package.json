{
  "name": "sudoku",
  "version": "1.0.0",
  "description": "A sudoku solver webapp",
  "main": "build/app.js",
  "scripts": {
    "typings-install": "./node_modules/.bin/typings install",
    "build-typescript": "./node_modules/.bin/tsc -p . --outDir build",
    "build-webpack": "./node_modules/.bin/webpack",
    "sass": "sass stylesheets/main.scss dist/main.css",
    "build": "npm run build-typescript && npm run build-webpack && npm run sass",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-test": "npm run build && npm test",
    "postinstall": "npm run typings-install && npm run build",
    "start": "node ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lencse/sudoku.git"
  },
  "author": "Levente LÃ¶ki <leventeloki@gmail.com> (https://lokilevente.hu/)",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.0.35",
    "express": "^4.14.1",
    "typescript": "^2.1.6",
    "typings": "^2.1.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@types/underscore": "^1.7.36",
    "afsudoku": "^1.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "underscore": "^1.8.3"
  }
}
